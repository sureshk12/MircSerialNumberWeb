<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginValidate</servlet-name>
        <servlet-class>com.mirc.serialNumber.LoginValidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>validateSerialNumber</servlet-name>
        <servlet-class>com.mirc.serialNumber.validateSerialNumber</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>validateFactoryForm</servlet-name>
        <servlet-class>com.mirc.serialNumber.validateFactoryForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidateModelNumber</servlet-name>
        <servlet-class>com.mirc.serialNumber.ValidateModelNumber</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginValidate</servlet-name>
        <url-pattern>/LoginValidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>validateSerialNumber</servlet-name>
        <url-pattern>/validateSerialNumber</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>validateFactoryForm</servlet-name>
        <url-pattern>/validateFactoryForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidateModelNumber</servlet-name>
        <url-pattern>/ValidateModelNumber</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
